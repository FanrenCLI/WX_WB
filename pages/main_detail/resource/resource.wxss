.menu{
  background-color: #f4f4f4;
  border-bottom:  solid #e9e9e9;
  display: table;
  width: 100%;
}
.menu-list{
  justify-content: space-between;
  display:table-cell ;
  vertical-align: middle;
  height: 90rpx;
}
.content{
  display: flex;
  margin-left: auto;
  margin-right: auto;

}
.content input{
  width: 85%;
  background-color: #ffffff;
  height: 60rpx;
  font-size: 30rpx;
  margin-left: 40rpx;
  border-radius: 30rpx;
  
}
.content image{
  height:60rpx;
  width: 60rpx;
  margin-top: 0;
  margin-left: 0;
}


